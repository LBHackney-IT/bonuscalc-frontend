{
  "name": "bonuscalc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 5000",
    "build": "next build",
    "prettier:fix": "npx prettier --write .",
    "prettier:test": "npx prettier -c .",
    "prestart": "next build",
    "start": "next start -p $PORT",
    "tests": "yarn test:unit && yarn test:e2e",
    "test:e2e": "PORT=5001 NEXT_PUBLIC_ENV_NAME=test start-test 5001 cy:run",
    "test:unit": "TZ=Europe/London node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "lint": "next lint",
    "e2e:audit": "PORT=5001 NEXT_PUBLIC_ENV_NAME=test start-test 5001 cy:run:audit",
    "e2e:interactive": "PORT=5001 NEXT_PUBLIC_ENV_NAME=test start-test 5001 cy:open",
    "e2e:server": "PORT=5001 NEXT_PUBLIC_ENV_NAME=test start-test 5001",
    "cy:run": "TZ=Europe/London cypress run --browser chrome --headless",
    "cy:run:audit": "TZ=Europe/London cypress run --browser chrome --headless --env runAudit=true",
    "cy:open": "TZ=Europe/London cypress open"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "axios": "^0.21.4",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lbh-frontend": "^3.6.1",
    "next": "11.1.2",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.16.1",
    "restana": "^4.9.1",
    "sass": "^1.39.2",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "babel-jest": "^27.2.0",
    "cypress": "^8.5.0",
    "cypress-audit": "^1.0.0",
    "cypress-dotenv": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "fishery": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-clean-console-reporter": "^0.3.0",
    "lint-staged": "^11.1.2",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.4.0",
    "start-server-and-test": "^1.14.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --fix",
    "**/*.{js,jsx,css,scss,md}": "prettier --write --list-different"
  }
}
