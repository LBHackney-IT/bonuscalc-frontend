{
  "name": "bonuscalc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5001",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 5000",
    "build": "next build",
    "prettier:fix": "prettier --write .",
    "prettier:test": "prettier -c .",
    "prestart": "next build",
    "start": "next start -p 5001",
    "tests": "yarn test:unit && yarn test:e2e",
    "test:e2e": "NODE_ENV=test start-test 5001 cy:run",
    "test:unit": "jest --runInBand --logHeapUsage",
    "lint": "next lint",
    "e2e:audit": "NODE_ENV=test start-test 5001 cy:run:audit",
    "e2e:interactive": "NODE_ENV=test start-test 5001 'cypress open'",
    "e2e:server": "NODE_ENV=test start-test 5001",
    "cy:run": "cypress run --browser chrome --headless",
    "cy:run:audit": "cypress run --browser chrome --headless --env runAudit=true",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@sentry/nextjs": "^6.16.1",
    "axios": "^0.25.0",
    "classnames": "^2.5.1",
    "cookie": "^0.4.1",
    "csv": "^6.0.5",
    "dayjs": "^1.10.7",
    "dotenv-flow": "^3.2.0",
    "govuk-frontend": "^3.14.0",
    "http-status-codes": "^2.1.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "9",
    "jspdf": "3",
    "lbh-frontend": "^3.6.1",
    "loglevel": "^1.8.0",
    "next": "13",
    "notifications-node-client": "7",
    "qs": "^6.10.1",
    "react": "18",
    "react-dom": "18",
    "react-hook-form": "^7.16.1",
    "restana": "^4.9.1",
    "sass": "^1.39.2",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0",
    "svg-pathdata": "^6.0.3",
    "swr": "^1.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@next/eslint-plugin-next": "13",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.0",
    "babel-jest": "^27.2.0",
    "canvas": "^2.9.0",
    "cypress": "^9.3.1",
    "cypress-audit": "^1.0.0",
    "cypress-dotenv-flow": "^1.2.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "13",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "fishery": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "nock": "^13.2.2",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.4.0",
    "start-server-and-test": "^1.14.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --fix",
    "**/*.{js,jsx,css,scss,md}": "prettier --write --list-different"
  }
}
