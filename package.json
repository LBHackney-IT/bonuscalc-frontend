{
  "name": "bonuscalc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api": "node -r dotenv/config api.js",
    "dev": "next dev -p 5000",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 5000",
    "build": "next build",
    "prettier:fix": "npx prettier --write .",
    "prettier:test": "npx prettier -c .",
    "prestart": "next build",
    "start": "next start -p $PORT",
    "test:unit": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "lint": "eslint . --max-warnings 0 --ext .js,.jsx && echo 'Lint complete.'"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "lbh-frontend": "^3.6.1",
    "next": "11.1.2",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "restana": "^4.9.1",
    "sass": "^1.39.2",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "fishery": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-clean-console-reporter": "^0.3.0",
    "json-server": "^0.16.3",
    "lint-staged": "^11.1.2",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.4.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --fix",
    "**/*.{js,jsx,css,scss,md}": "prettier --write --list-different"
  }
}
